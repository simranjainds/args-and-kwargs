def funargs(normal,*args,**kwargs):
    print(normal)
    for item in args:
        print(item)
    print("now is the time to introduce hobby")
    for key, value in kwargs.items():
        print(f"person name is {key} and its fav game is {value}")
har=["simran","gaurang","meena","rakesh"]
simi={"simran":"tt","gaurang":"basketball","meena":"carrom","rakesh":"batminton"}
normal="4 members in the family"
funargs(normal,*har,**simi)
